<main class="px-3">
  <h1>Welcome</h1>
  <br>

    <!-- Element to contain animated typing -->
  <p class="lead fs-4"><span id="element"></span></p>

  <!-- Load library from the CDN -->
  <script src="https://unpkg.com/typed.js@2.1.0/dist/typed.umd.js"></script>

  <!-- Setup and start animation! -->
  <script>
    var typed = new Typed('#element', {
      strings: ["Software development shouldn't be a secret process, when I take on a new project I make it my goal to not only deliver a well crafted product but also to ensure that every stage of the process is full communicated."],
      typeSpeed: 40,
    });
  </script>
</body>


  <p class="lead">
    <%= link_to "About Me", about_path, class: "btn btn-lg btn-light fw-bold border-white bg-white" %>
  </p>
</main>
